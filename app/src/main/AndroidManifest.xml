<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.muninn.sudoku">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sudoku_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sudoku_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.muninn.sudoku.FinalScreen"></activity>
        <activity android:name="com.muninn.sudoku.HardBoard" />
        <activity android:name="com.muninn.sudoku.MediumBoard" />
        <activity android:name="com.muninn.sudoku.EasyBoard" />
        <activity android:name="com.muninn.sudoku.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>